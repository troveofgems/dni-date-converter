{
  "name": "dni-cavern-time",
  "version": "1.0.0",
  "homepage": "https://github.com/troveofgems/dni-date-converter#readme",
  "description": "D'ni Date-Time Converter Package",
  "keywords": [
    "dni time",
    "myst dni time",
    "d'ni time",
    "d'ni",
    "dni time manipulation",
    "time manipulation",
    "dni date handling",
    "dni cavern time conversion",
    "game development",
    "npm package",
    "javascript",
    "typescript",
    "node"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc && cp LICENSE dist && cp README.md dist && cp package.json dist",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "ts-node src/index.ts",
    "test": "jest --config jest.config.ts && npm run ucr",
    "ucr": "rm -rf docs/* && cp -r coverage/lcov-report/* docs",
    "push-patch": "npm version patch && git push",
    "push-minor": "npm version minor && git push",
    "push-major": "npm vresion minor && git push",
    "deploy-patch": "npm run test && npm run build && npm run push-patch && npm run deploy-to-npm-registry",
    "deploy-minor": "npm run test && npm run build && npm run push-minor && npm run deploy-to-npm-registry",
    "deploy-major": "npm run test && npm run build && npm run push-major && npm run deploy-to-npm-registry",
    "deploy-to-npm-registry": "npm publish"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@types/big.js": "^6.2.2",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "big.js": "^6.2.1"
  },
  "author": "Dustin K Greco <dkgreco@thetroveofgems.tech> (https://www.thetroveofgems.tech/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troveofgems/dni-date-converter.git"
  },
  "dist-tags": {
    "latest": "https://github.com/troveofgems/dni-date-converter/releases/tag/v1.0.0"
  },
  "bugs": {
    "url": "https://github.com/troveofgems/dni-date-converter/issues",
    "email": "dkgreco@thetroveofgems.tech"
  },
  "license": "MIT"
}
